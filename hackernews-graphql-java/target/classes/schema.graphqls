type Query {
  allDesigns: [Design]
}

type Mutation {
  createDesign(url: String!, description: String!): Design
  deleteDesign(id: String!): Boolean
  editDesign(id: String!, url: String, description: String): Design
}

type Design {
  id: ID!
  url: String!
  description: String!
}

schema {
  query: Query
  mutation: Mutation
}
